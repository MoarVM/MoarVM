.SUFFIXES: .c @obj@

moarvm@exe@: $(OBJECTS) @thirdpartylibs@
	$(LD) @ldout@$@ $(LDFLAGS) $(OBJECTS) $(LDLIBS)

apr: @aprlib@

libuv: _uvlib_

$(OBJECTS): $(HEADERS)

.c@obj@:
	$(CC) @ccswitch@ $(CFLAGS) $(CINCLUDES) @ccout@$@ $*.c

src/strings/unicode.c: src/strings/unicode_db.c src/strings/unicode_ops.c
	$(CAT) src/strings/unicode_db.c >$@
	$(CAT) src/strings/unicode_ops.c >>$@

3rdparty/apr/include/apr.h: @aprlib@

@aprlib@:
	@aprrule@

_uvlib_: _uvobjects_
	_uvrule_

@laolib@:
	@laorule@

@tomlib@: $(TOM_OBJECTS)
	@tomrule@

@shalib@: @shaobjects@
	@sharule@

clean:
	-$(RM) $(OBJECTS)
	@clean@
	@aprclean@
#	_uvclean_
	@laoclean@
	@tomclean@
	@shaclean@

realclean: clean
	-$(RM) moarvm@exe@
	-$(RM) Makefile src/gen/config.h src/strings/unicode.c
