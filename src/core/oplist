decoderempty        w(int64) r(obj) :pure
indexingoptimized   w(str) r(str) :pure
captureinnerlex     r(obj) :noinline
unicmp_s            w(int64) r(str) r(str) r(int64) r(int64) r(int64) :pure
DEPRECATED_92       r(obj) r(obj)
